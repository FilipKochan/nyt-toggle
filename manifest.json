{
  "name": "NYT Sudoku Toggle",
  "description": "Toggle between normal and candidate mode with key press in NYT sudoku game.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "hello.html"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "js": ["nyt-toggle.js"],
      "matches": [
        "https://www.nytimes.com/puzzles/sudoku",
        "https://www.nytimes.com/puzzles/sudoku/*"
      ]
    }
  ]
}
